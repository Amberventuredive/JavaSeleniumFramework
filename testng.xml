<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="3" parallel="tests" name="test on browserstack">

<listeners>
		<listener
			class-name="com.learnautomation.utility.ExtentTestNGITestListener" />
	</listeners>

  <test name="Testonwindow11">
  
       <parameter name="cloud" value="true"/>
       <parameter name="os" value="Windows"/>
       <parameter name="os_version" value="11"/>
       <parameter name="browser" value="Chrome"/>
       <parameter name="browser_version" value="latest"/>
  
  
         <classes>
			<class name="com.learnautomation.testcases.LoginTest" />
		</classes>
  </test> 
  
  <test name="Testonwindow10">
  
       <parameter name="cloud" value="true"/>
       <parameter name="os" value="Windows"/>
       <parameter name="os_version" value="10"/>
       <parameter name="browser" value="Firefox"/>
       <parameter name="browser_version" value="latest"/>
  
  
         <classes>
			<class name="com.learnautomation.testcases.LoginTest" />
		</classes>
  </test> 
</suite> 
